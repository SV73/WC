A)callback_example.js
/* Caller Function */
const addNum = (a, b, adab) => {
    setTimeout(() => {
        adab(a + b);
    }, 2000);
};

// Calling Function
addNum(4, 5, (c) => {
    console.log(c);
});


B)event_loop_example.js
setTimeout(function timer() {
    console.log('2 seconds over, You clicked the button!');
}, 2000);

console.log("hi!");

setTimeout(function timeout() {
    console.log('5 seconds over, You clicked the button!');
}, 5000);

console.log("Welcome to event loops");
